{
  "productName": "electric-scan",

  "directories": {
    "output": "release/"
  },
  "files": [
      "**/*",
      "!**/*.ts",
      "!*.code-workspace",
      "!README.md",
      "!BUILD.md",
      "!browserlist",
      "!package.json",
      "!package-lock.json",
      "!src/",
      "!e2e/",
      "!hooks/",
      "!angular.json",
      "!_config.yml",
      "!karma.conf.js",
      "!tsconfig.json",
      "!tslint.json"
  ],
  "win": {
    "icon": "dist",
    "target": [
      "portable"
    ]
  },
  "mac": {
    "icon": "dist",
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "icon": "dist",
    "target": [
      "deb",
      "rpm"
    ]
  }
}
